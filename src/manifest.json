{
  "name": "Awesome Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "No Description Needed for Awesome Extension",
  "icons": {
    "128": "assets/cool-doge.png"
  },
  "devtools_page": "devtool/devtool.html",
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "background": {
    "scripts": ["background-script.js"]
  },
  "browser_action": {
    "default_icon": "assets/cool-doge.png",
    "default_title": "Go Awesome!",
    "default_popup": "browser-action/index.html"
  }
}